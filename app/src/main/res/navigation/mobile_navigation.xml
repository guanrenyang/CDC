<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.cdc.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_data_processing"
        android:name="com.example.cdc.ui.dataProcessing.DataProcessingFragment"
        android:label="@string/title_data_processing"
        tools:layout="@layout/fragment_data_processing" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.cdc.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_information_report"
        android:name="com.example.cdc.ui.InformationReportFragment"
        android:label="@string/title_information_submit"
        tools:layout="@layout/fragment_information_report" />
</navigation>